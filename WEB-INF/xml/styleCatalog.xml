<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE StyleCatalog SYSTEM "styleCatalog.dtd">
<StyleCatalog>
	<TransformDefns>
		<TransformDefn class="com.bdl.mstr.GeodashAppViewTransform" name="GeodashAppViewTransform" />
		<TransformDefn class="com.bdl.mstr.GeodashActionTransform" name="GeodashActionTransform" />
		<TransformDefn class="com.bdl.mstr.GeodashRWAppViewTransform" name="GeodashRWAppViewTransform" />
		<TransformDefn class="com.bdl.mstr.GeodashRWActionTransform" name="GeodashRWActionTransform" />
		<TransformDefn class="com.bdl.mstr.GeodashGridTransform" name="GeodashGridTransform" />
	</TransformDefns>
	<Styles>
		<Style description="Style to render the Geodash3 Visualization" name="Geodash3MojoVisualizationStyle" transform="ReportAjaxMojoVisualizationTransform">
			<ActualParameters>
				<ActualParameter name="documentStyle" type="1" value="RWGridJsonStyle" />
				<ActualParameter name="reportXMLStyle" type="1" value="Geodash3VisualizationMojoDataStyle" />
				<ActualParameter name="mojoClassName" type="1" value="plugins.Geodash3.Geodash3Vis" />
			</ActualParameters>
			<Layouts>
				<Layout layout_source="AppLayoutSourceFile"><![CDATA[/WEB-INF/xml/layouts/MojoVisualizationLayout.xml]]></Layout>
			</Layouts>
		</Style>
		<Style description="Style for rendering data for Javascript side" name="Geodash3VisualizationMojoDataStyle" transform="MojoVisualizationDataTransform">
			<ActualParameters>
				<ActualParameter name="contentType" type="1" value="JSON" />
				<!-- parameter below is need to get information about currently selected element -->
				<ActualParameter name="includeGridSelectorCurrentElements" type="4" value="true" />
			</ActualParameters>
		</Style>



		<Style description="Renders the main geodash view constructs including css and script files." name="GeodashAppViewStyle" transform="GeodashAppViewTransform">
			<Layouts>
				<Layout layout_source="AppLayoutSourceFile"><![CDATA[@Geodash2:/WEB-INF/xml/layouts/GeodashLayout.xml]]></Layout>
			</Layouts>
		</Style>
		<Style description="Responds to geodash action requests." name="GeodashActionStyle" transform="GeodashActionTransform" />
		<Style description="Generates the geodash app for an RW document." name="GeodashRWAppViewStyle" transform="GeodashRWAppViewTransform">
			<Layouts>
				<Layout layout_source="AppLayoutSourceFile"><![CDATA[@Geodash2:/WEB-INF/xml/layouts/GeodashRWLayout.xml]]></Layout>
			</Layouts>
		</Style>
		<Style description="Responds to action requests from a geodash app within an RW document." name="GeodashRWActionStyle" transform="GeodashRWActionTransform" />
		<Style description="Geodash Grid main display" name="GeodashGridStyle" transform="GeodashGridTransform">
			<Layouts>
				<Layout layout_source="AppLayoutSourceFile"><![CDATA[@Geodash2:/WEB-INF/xml/layouts/GeodashGridLayout.xml]]></Layout>
			</Layouts>
		</Style>

	</Styles>
</StyleCatalog>
